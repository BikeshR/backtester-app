{
  "name": "backtester-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .jsx --ext .js src/",
    "start": "env-cmd debug nodemon --exec babel-node src/server/app.js",
    "build": "npm run build-server && npm run build-client",
    "build-client": "rm -rf dist/client && env-cmd buildclient webpack --progress --config src/webpack.production.js",
    "build-server": "rm -rf dist/server && mkdir -p dist/server && env-cmd buildserver babel ./src/server -s -d ./dist/server/main",
    "test": "env-cmd test jest src/ --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BikeshR/backtester-app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BikeshR/backtester-app/issues"
  },
  "homepage": "https://github.com/BikeshR/backtester-app#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-syntax-json-strings": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "brotli-gzip-webpack-plugin": "^0.5.0",
    "env-cmd": "^8.0.2",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-each": "^24.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "webpack": "^4.41.3",
    "webpack-command": "^0.5.0",
    "webpack-dev-middleware": "^3.7.2",
